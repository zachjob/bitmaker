<%= form_for(@project, html: {multipart: true}) do |f| %>

<% if @user.errors.any? %>
    <ul>
      <% @user.errors.full_messages.each do |m| %>
      <li><%= m %></li>
      <% end %>
    </ul>
  <% end %>
  
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div>
    <%= f.label :goal %>
    <%= f.number_field :goal %>
  </div>

  <div>
    <%= f.label :image %>
    <%= f.text_field :image %>
  </div>

  <div>
    <%= f.label :start_date %>
    <%= f.datetime_select :start_date %>
  </div>

  <div>
    <%= f.label :end_date %>
    <%= f.datetime_select :end_date %>
  </div>

  <%= f.submit %>
<% end %>
