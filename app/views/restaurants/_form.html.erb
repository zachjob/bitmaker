<%= form_for restaurant do |f| %>

<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :address %>
  <%= f.text_field :address %>
</p>

<p>
  <%= f.label :open_time %>
  <%= f.text_field :open_time %>
</p>

<p>
  <%= f.label :close_time %>
  <%= f.text_field :close_time %>
</p>

<p>
  <%= f.label :seats %>
  <%= f.text_field :seats %>
</p>

<p>
  <%= f.label :price_range %>
  <%= f.select(:price_range, (1..5).to_a) %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
